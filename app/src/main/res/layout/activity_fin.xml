<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activitys.ActivityFin">

    <EditText
        android:id="@+id/fldGraduaciones"
        android:layout_width="391dp"
        android:layout_height="39dp"
        android:ems="10"
        android:hint="Graduaciones (separar en comas)"
        android:inputType="textPersonName"
        android:textAlignment="center"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="550dp" />

    <EditText
        android:id="@+id/fldVehiculosRetenidos"
        android:layout_width="158dp"
        android:layout_height="43dp"
        android:ems="10"
        android:hint="Vehiculos retenidos"
        android:inputType="number"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/fldLicenciasRetenidas"
        tools:layout_editor_absoluteY="380dp" />

    <EditText
        android:id="@+id/fldLicenciasRetenidas"
        android:layout_width="158dp"
        android:layout_height="43dp"
        android:ems="10"
        android:hint="Licencias retenidas"
        android:inputType="number"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/fldVehiculosRetenidos"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="380dp" />

    <EditText
        android:id="@+id/fldInfraccionados"
        android:layout_width="126dp"
        android:layout_height="39dp"
        android:ems="10"
        android:hint="Infraccionados"
        android:inputType="number"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/fldControlados"
        tools:layout_editor_absoluteY="324dp" />

    <EditText
        android:id="@+id/fldControlados"
        android:layout_width="126dp"
        android:layout_height="39dp"
        android:ems="10"
        android:hint="Controlados"
        android:inputType="number"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@+id/fldInfraccionados"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="324dp" />

    <EditText
        android:id="@+id/fldHorarioInicio"
        android:layout_width="69dp"
        android:layout_height="33dp"
        android:ems="10"
        android:hint="Inicio"
        android:inputType="number"
        android:textSize="12sp"
        app:layout_constraintEnd_toStartOf="@+id/fldHorarioFin"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/txtHorario"
        tools:layout_editor_absoluteY="51dp" />

    <TextView
        android:id="@+id/txtHorario"
        android:layout_width="68dp"
        android:layout_height="26dp"
        android:text="Horario"
        app:layout_constraintEnd_toStartOf="@+id/fldHorarioInicio"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="58dp" />

    <EditText
        android:id="@+id/fldHorarioFin"
        android:layout_width="76dp"
        android:layout_height="33dp"
        android:ems="10"
        android:hint="Fin"
        android:inputType="number"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/fldHorarioInicio"
        tools:layout_editor_absoluteY="51dp" />

    <EditText
        android:id="@+id/fldDominiosMovilesFin"
        android:layout_width="367dp"
        android:layout_height="42dp"
        android:ems="10"
        android:hint="Dominios de los moviles"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="227dp" />

    <TextView
        android:id="@+id/txtAgentesFin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Agentes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="107dp" />

    <EditText
        android:id="@+id/fldApellidosFin"
        android:layout_width="367dp"
        android:layout_height="42dp"
        android:ems="10"
        android:hint="Apellidos de los agentes"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="139dp" />

    <TextView
        android:id="@+id/txtMovilesFin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Moviles"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="192dp" />

    <EditText
        android:id="@+id/fldObservacionesFin"
        android:layout_width="391dp"
        android:layout_height="39dp"
        android:ems="10"
        android:gravity="start|top"
        android:hint="Observaciones"
        android:inputType="textMultiLine"
        android:textAlignment="center"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.45"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="624dp" />

    <TextView
        android:id="@+id/txtObservacionesFin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Observaciones"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="597dp" />

    <TextView
        android:id="@+id/txtDatosOperativo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Datos del operativo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="285dp" />

    <TextView
        android:id="@+id/txtMotivos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Motivos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="444dp" />

    <EditText
        android:id="@+id/fldMotivos"
        android:layout_width="391dp"
        android:layout_height="39dp"
        android:ems="10"
        android:hint="Motivos (separar en comas)"
        android:inputType="textPersonName"
        android:textAlignment="center"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="477dp" />

    <TextView
        android:id="@+id/txtAlcoholemiasPositivas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Alcoholemias positivas "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="524dp" />

    <Button
        android:id="@+id/btnGenerarInformeFin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="generar informe"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="669dp" />

</androidx.constraintlayout.widget.ConstraintLayout>